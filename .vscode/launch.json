    "version": "0.2.0",
    "configurations": [
        {
            "name": "BrainBench DEV",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--dart-define=FIREBASE_ENV=dev"
            ],
            "preLaunchTask": "use-firebase-dev"
        },
        {
            "name": "BrainBench TEST",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--dart-define=FIREBASE_ENV=test"
            ],
            "preLaunchTask": "use-firebase-test"
        },
        {
            "name": "BrainBench PROD",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--dart-define=FIREBASE_ENV=prod"
            ],
            "preLaunchTask": "use-firebase-prod"
        }
    ]
}